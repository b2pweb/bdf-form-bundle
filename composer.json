{
  "name": "b2pweb/bdf-form-bundle",
  "description": "Bundle for BDF Form",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Vincent Quatrevieux"
    }
  ],
  "autoload": {
    "psr-4": { "Bdf\\Form\\Bundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.2",
    "b2pweb/bdf-form": "~1.0",
    "symfony/config": "^4.0|^5.0",
    "symfony/dependency-injection": "^4.0|^5.0",
    "symfony/framework-bundle": "^4.0|^5.0",
    "symfony/service-contracts": "^1.1|^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0|~9.0",
    "symfony/yaml": "~4.0|^5.0",
    "symfony/security-csrf": "~4.0|~5.0"
  },
  "suggest": {
    "b2pweb/bdf-form-attribute": "To use PHP 8 attributes syntax for forms"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "tests": "phpunit"
  }
}
